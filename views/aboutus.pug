                 img.img-thumbnail(src='assets/images/f4.jpeg', alt='')
             .grid-item.fin(data-category='fin')
               a.link(href='assets/images/f5.jpeg', data-fancybox='images')
                 span.content
                   span.content-title Financial Sector
                   span.content-edu
                     i.icons.fa.fa-search-plus
                     span Photoghaphy Master
                 img.img-thumbnail(src='assets/images/f5.jpeg', alt='')
             .grid-item.edu(data-category='edu')
               a.link(href='assets/images/e4.png', data-fancybox='images')
                 span.content
                   span.content-title Educational Sector
                   span.content-edu
                     i.icons.fa.fa-search-plus
                     span Photoghaphy Master
                 img.img-thumbnail(src='assets/images/e4.png', alt='')
             .grid-item.teacher.edu(data-category='edu')
               a.link(href='assets/images/e1.png', data-fancybox='images')
                 span.content
                   span.content-title Educational Sector
                   span.content-edu
                     i.icons.fa.fa-search-plus
                     span Photoghaphy Master
