                   span.content-title Educational Sector
                   span.content-edu
                     i.icons.fa.fa-search-plus
                     span Photoghaphy Master
                 img.img-thumbnail(src='assets/images/e3.jpeg', alt='')
             .grid-item.edu(data-category='edu')
               a.link(href='assets/images/e2.jpeg', data-fancybox='images')
                 span.content
                   span.content-title Educational Sector
                   span.content-edu
                     i.icons.fa.fa-search-plus
                     span Photoghaphy Master
                 img.img-thumbnail(src='assets/images/e2.jpeg', alt='')
 // SLIDER TALK ABOUT US
 .section.background-opacity.slider-talk-about-us
   .container
     #people-talk.slider-talk-about-us-wrapper.carousel.slide(style='height: 500px')
       .slider-talk-about-us-content.carousel-inner(role='listbox')
         .peopel-item.item.active
           h2(style='text-shadow: -5px -5px 2px #ef5734') &quot;Do Creative, Be Developing&quot;
         //BUTTON BACK TO TOP
         #back-top
           a(href='#top')
             i.fa.fa-angle-double-up
         // LOADING
         .body-2.loading
            .dots-loader
 include includes/foot
 include includes/scripts
 script(src='assets/libs/timeline-plugin/js/timeline.min.js')
